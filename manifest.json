{
  "manifest_version": 2,

  "name": "No One Extension",
  "description": "Removes all mentions of a list of names",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["js/lib/jquery-3.1.1.min.js", "js/noex.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}